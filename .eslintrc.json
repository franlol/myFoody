{
    "extends": "standard",
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {
        "document": false,
        "window": false,
        "console": false,
        "$": false,
        "FileReader": false
    },
    "rules": {
        "indent": [
            2,
            4
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}